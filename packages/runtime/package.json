{
  "name": "@stacksolo/runtime",
  "version": "0.1.4",
  "description": "Runtime utilities for StackSolo applications",
  "repository": {
    "type": "git",
    "url": "https://github.com/monkeybarrels/stacksolo.git",
    "directory": "packages/runtime"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "firebase-admin": "^12.0.0 || ^13.0.0",
    "@google-cloud/pubsub": "^4.0.0",
    "@google-cloud/storage": "^7.0.0",
    "@google-cloud/secret-manager": "^5.0.0",
    "nats": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "firebase-admin": {
      "optional": true
    },
    "@google-cloud/pubsub": {
      "optional": true
    },
    "@google-cloud/storage": {
      "optional": true
    },
    "@google-cloud/secret-manager": {
      "optional": true
    },
    "nats": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@google-cloud/secret-manager": "^5.0.0",
    "nats": "^2.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "stacksolo",
    "runtime",
    "firebase",
    "gcp"
  ],
  "license": "MIT"
}
