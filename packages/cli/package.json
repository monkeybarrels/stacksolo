{
  "name": "@stacksolo/cli",
  "version": "0.1.13",
  "description": "CLI for StackSolo - deploy GCP infrastructure from the command line",
  "repository": {
    "type": "git",
    "url": "https://github.com/monkeybarrels/stacksolo.git",
    "directory": "packages/cli"
  },
  "homepage": "https://stacksolo.dev",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "stacksolo": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@stacksolo/core": "workspace:*",
    "@stacksolo/plugin-gcp-cdktf": "workspace:*",
    "better-sqlite3": "^11.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "inquirer": "^9.2.0",
    "kysely": "^0.27.0",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@stacksolo/blueprint": "workspace:*",
    "@stacksolo/registry": "workspace:*",
    "@stacksolo/shared": "workspace:*",
    "@types/better-sqlite3": "^7.6.0",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.19.27",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "cli",
    "stacksolo",
    "infrastructure",
    "gcp",
    "google-cloud",
    "terraform",
    "cdktf",
    "deploy",
    "cloud-functions",
    "cloud-run"
  ],
  "engines": {
    "node": ">=18"
  },
  "license": "MIT"
}
