{
  "name": "@stacksolo/blueprint",
  "version": "0.1.1",
  "description": "Config parser and code generator for StackSolo infrastructure",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
